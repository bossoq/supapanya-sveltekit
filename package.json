{
  "name": "simplesveltekittemplate",
  "description": "A simple SvelteKit template with TailwindCSS and TypeScript",
  "author": "Kittipos Wajakajornrit",
  "repository": "https://github.com/bossoq/simple-sveltekit-template",
  "version": "1.2.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^22.8.4",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "postcss": "^8.4.21",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.5",
    "svelte-multiselect": "^11.0.0-rc.1",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "type": "module",
  "dependencies": {
    "@google-apps/meet": "^0.3.0",
    "@prisma/client": "^5.21.1",
    "@tiptap/core": "^2.9.1",
    "@tiptap/extension-bubble-menu": "^2.9.1",
    "@tiptap/extension-focus": "^2.9.1",
    "@tiptap/extension-image": "^2.9.1",
    "@tiptap/extension-link": "^2.9.1",
    "@tiptap/extension-table": "^2.9.1",
    "@tiptap/extension-table-cell": "^2.9.1",
    "@tiptap/extension-table-header": "^2.9.1",
    "@tiptap/extension-table-row": "^2.9.1",
    "@tiptap/extension-text-align": "^2.9.1",
    "@tiptap/extension-underline": "^2.9.1",
    "@tiptap/html": "^2.9.1",
    "@tiptap/pm": "^2.9.1",
    "@tiptap/starter-kit": "^2.9.1",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "bcryptjs": "^2.4.3",
    "google-auth-library": "^9.14.2",
    "hls.js": "^1.5.17",
    "jwt-simple": "^0.5.6",
    "media-icons": "^1.1.5",
    "prisma": "^5.21.1",
    "remixicon": "^4.4.0",
    "sharp": "^0.33.5",
    "svelte-flatpickr": "^3.3.4",
    "vidstack": "^1.12.11"
  }
}
